		<infoclass>
			<id>12</id>
			<name>白银</name>
			<class>00000000000100010001000100010006</class>
			<selectable>true</selectable>
		</infoclass>
		<infoclass>
			<id>13</id>
			<name>贵金属</name>
			<class>00000005000D0000000S000A</class>
			<selectable>true</selectable>
		</infoclass>
        <infoclass>
            <id>200</id>
            <name>跑马屏</name>
