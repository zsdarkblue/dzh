    <!--滚动新闻过滤-->
    <pushinfo>
        <RoundSetting>01,02,03,04,05</RoundSetting>
        <!--滚动资讯过滤-->
        <NewsLang>1</NewsLang>
        <!--语言-->
        <PushRoundInfoMax>100</PushRoundInfoMax>
        <!--保存最大个数-->
        <PushInfoUIServer>dzhd://DZH2/news/nBrowse.dzh.ui?</PushInfoUIServer>
    </pushinfo>
    <!--短信精灵过滤-->
    <notice>
        <NoticeFilter>49155</NoticeFilter>
        <!--默认没有-->
    </notice>
    <!--即时信息屏蔽-->
    <message>
        <IMshow>1</IMshow>
    </message>
	<!--实时观察-->
    <comment>
        <Request>/sys/observation?@=nobleMetal.pop&amp;__timeout=-1</Request>
    </comment>
</newsfilter>
