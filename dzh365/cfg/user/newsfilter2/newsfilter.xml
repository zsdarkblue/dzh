<?xml version="1.0" encoding="GB2312" ?>
<newsfilter>
    <!--所有新闻支持的DID-->
    <infoclasses>
        <infoclass>
            <id>01</id>
            <name>股票市场</name>
            <class>0000000000010000</class>
            <selectable>true</selectable>
        </infoclass>
        <infoclass>
            <id>02</id>
            <name>商品市场</name>
            <class>0000000000010001</class>
            <selectable>true</selectable>
        </infoclass>
        <infoclass>
            <id>03</id>
            <name>外汇市场</name>
            <class>0000000000010004</class>
            <selectable>true</selectable>
        </infoclass>
        <infoclass>
            <id>04</id>
            <name>货币市场</name>
            <class>0000000000010006</class>
            <selectable>true</selectable>
        </infoclass>
        <infoclass>
            <id>05</id>
            <name>债券市场</name>
            <class>0000000000010002</class>
            <selectable>true</selectable>
        </infoclass>
        <infoclass>
            <id>19</id>
            <name>大智慧产品</name>
            <class>0000000300020000</class>
            <selectable>false</selectable>
        </infoclass>
        <infoclass>
            <id>200</id>
            <name>跑马屏</name>
            <class>0000000300020008</class>
            <selectable>ture</selectable>
        </infoclass>
        <infoclass>
            <id>21</id>
            <name>市场数据</name>
            <class>0000000300020003</class>
            <selectable>false</selectable>
        </infoclass>
        <infoclass>
            <id>22</id>
            <name>实时解盘</name>
            <class>0000000300020004</class>
            <selectable>false</selectable>
        </infoclass>
    </infoclasses>
    <!--滚动新闻过滤-->
    <pushinfo>
        <RoundSetting>02,03,04,05,200,</RoundSetting>
        <!--滚动资讯过滤-->
        <NewsLang>1</NewsLang>
        <!--语言-->
        <PushRoundInfoMax>100</PushRoundInfoMax>
        <!--保存最大个数-->
        <PushInfoUIServer>dzhd://DZH2/news/nBrowse.dzh.ui?</PushInfoUIServer>
        <!--以下是单条讯息弹出页的配置，其中Category意义如下：
			1新闻，2法律法规，3公司公告，4历史公告，5权证公告，6港股公告
			7基金公告，8机构公告，9研报，10债券公告，11微博，12论坛，13老资讯后台
			14集合理财公告，15信托计划公告，16保险产品公告，17银行个人理财公告
			18招聘，19广告，101新闻(英文)，102法律法规(英文)
		-->
        <PushInfoHTMLServer_1>root://news/details/index.html#{Id}#DZHPOPUP(854,504)#</PushInfoHTMLServer_1>
        <PushInfoHTMLServer_3>root://news/details/notice.html#{Id}-3#DZHPOPUP(854,504)#</PushInfoHTMLServer_3>
        <PushInfoHTMLServer_6>root://news/details/notice.html#{Id}-6#DZHPOPUP(854,504)#</PushInfoHTMLServer_6>
        <PushInfoHTMLServer_7>root://news/details/notice.html#{Id}-7#DZHPOPUP(854,504)#</PushInfoHTMLServer_7>
        <PushInfoHTMLServer_9>root://news/details/report.html#{Id}#DZHPOPUP(854,504)#</PushInfoHTMLServer_9>
        <PushInfoHTMLServer_10>root://news/details/notice.html#{Id}-10#DZHPOPUP(854,504)#</PushInfoHTMLServer_10>
        <PushInfoHTMLServer_14>root://news/details/notice.html#{Id}-14#DZHPOPUP(854,504)#</PushInfoHTMLServer_14>
        <PushInfoHTMLServer_17>root://news/details/notice.html#{Id}-17#DZHPOPUP(854,504)#</PushInfoHTMLServer_17>
    </pushinfo>
    <!--短信精灵过滤-->
    <notice>
        <NoticeFilter>16564224</NoticeFilter>
        <!--默认没有-->
    </notice>
    <!--即时信息屏蔽-->
    <message>
        <IMshow>1</IMshow>
    </message>
</newsfilter>
