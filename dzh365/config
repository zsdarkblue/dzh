log4j.rootLogger=OFF, File

log4j.appender.Console=org.apache.log4j.ConsoleAppender
log4j.appender.Console.layout=org.apache.log4j.PatternLayout
log4j.appender.Console.layout.ConversionPattern=%-5p %d{%H:%M:%S} %m%n

# log4j.appender.Win32=org.apache.log4j.OutputDebugStringAppender
# log4j.appender.Win32.layout=org.apache.log4j.PatternLayout
# log4j.appender.Win32.layout.ConversionPattern=%d{%Y-%m-%d %H:%M:%S} %-5p - %m%n

log4j.appender.File=org.apache.log4j.RollingFileAppender
log4j.appender.File.layout=org.apache.log4j.PatternLayout
log4j.appender.File.layout.ConversionPattern=%-5p %d{%Y-%m-%d %H:%M:%S} %m%n
log4j.appender.File.File=peer.log
log4j.appender.File.Append=true

p2p.proxy.port = 1080
